//Cedestinson Ductan
public class MultiplicationTable {
	
	public static void main(String[]args) {
		// outside loop
		for(int i = 1; i <= 10; i++)
			
	  { 
			// inner loop
			for(int j = 1; j <= 10; j++) 
		  
		  
	  { 
		  // "\t"  uses an escape sequence.
				
		 System.out.printf(i*j+"\t");
		 
		      }
	  
		      System.out.println();
		      
		      }
	}
}
		  
		
